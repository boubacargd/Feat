{
  "expo": {
    "name": "feat_front",
    "slug": "feat_front",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "com.boubacargd.feat",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/images/app-icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "extra": {
      "googleOAuth": {
        "clientId": "594973443734-2ra4kj49bvu54no1outgv9fe64n3eq7v.apps.googleusercontent.com",
        "iosClientId": "594973443734-sjc76c8r96vodbtv8igamt9jbjr66g87.apps.googleusercontent.com",
        "androidClientId": "594973443734-5lh7t7mukvsnv6vfhr4vcikqd6kchlr6.apps.googleusercontent.com"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.boubacargd.feat" 
    },
    "android": {
      "package": "com.boubacargd.feat",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
